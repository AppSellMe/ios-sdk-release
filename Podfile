   workspace 'AppSellSDKShowcase'
   project 'ObjcShowcase.xcodeproj'

   pod 'AppSellSDK', :podspec => '~/xCode/AppSellSDK/'
end

target 'SwiftShowcase' do
   workspace 'AppSellSDKShowcase'
   project 'SwiftShowcase.xcodeproj'

   pod 'AppSellSDK', :podspec => '~/xCode/AppSellSDK/'
end